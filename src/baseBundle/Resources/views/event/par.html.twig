{% extends '@base/admin/base.html.twig' %}
{% block title %}Admin - Event{% endblock %}
{% block body %}
    <!-- Page Content -->
    <div class="container-fluid">
        <div class="card-header">
            <i class="fas fa-table"></i>
            Participation List
        </div>
        <div class="card-body">
            <div class="table-responsive table-hover">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                    <tr>
                        <th> Stand Id </th>
                        <th> Event Id </th>
                        <th> CIN </th>
                        <th> Name </th>
                        <th> Last Name </th>
                        <th> Phone Number </th>
                    </tr>
                    </thead>
                    <tbody>
                    {%  for e in aa %}
                        <tr>
                            <td> {{ e.idStand }} </td>
                            <td> {{ e.idEvenement }} </td>
                            <td> {{ e.cinParticipant }} </td>
                            <td> {{ e.nom }} </td>
                            <td> {{ e.prenom }} </td>
                            <td> {{ e.tel }} </td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
{% endblock %}